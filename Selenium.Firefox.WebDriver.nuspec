<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Selenium.Firefox.WebDriver</id>
    <version>0.16.1</version>
    <authors>jbaranda</authors>
    <summary>Selenium Firefox WebDriver Marionette (Win32)</summary>
    <description>
	Repurposed nuget package of Selenium.WebDriver.ChromeDriver but for Firefox Driver instead
	Install Firefox Driver(Win32) Marionette for Selenium WebDriver into your Unit Test Project.
	"geckodriver.exe" is copied to bin folder from package folder when the build process.
	NuGet package restoring ready, and no need to commit "geckodriver.exe" binary into source code control repository.
    </description>
    <language>en-US</language>
    <projectUrl>https://github.com/jbaranda/nupkg-selenium-webdrivers</projectUrl>
    <licenseUrl>http://unlicense.org</licenseUrl>
    <tags>Selenium WebDriver Firefox</tags>
    <releaseNotes>
	<![CDATA[
    v0.16.1
      - Firefox Driver 0.16.1 release
    v0.16.0
      - Firefox Driver 0.16.0 release
    v0.15.0
      - Firefox Driver 0.15.0 release
  	v0.14.0
      - Firefox Driver 0.14.0 release
  	v0.13.0
      - Firefox Driver 0.13.0 release
  	v0.12.0
      - Firefox Driver 0.12.0 release
	  v0.11.1
      - Firefox Driver 0.11.1 release
      ]]>
    </releaseNotes>
  </metadata>
  <files>
    <file src="geckodriver.exe" target="driver"/>
    <file src="Selenium.Firefox.WebDriver.targets" target="build"/>
  </files>
</package>
